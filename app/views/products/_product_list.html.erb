<div class="sub-title">
  <h2>Browse Our Collection</h2>
</div>
<div class="products-list">
  <% @products.each do |product| %>
    <%= render 'product_card', product: product %>
  <% end %>
</div>
<div class="pagination">
  <%= paginate @products, param_name: :products_page, params: @pagination_params %>
</div>